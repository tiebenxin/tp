<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:left="2dp"
        android:top="2dp">
        <shape>
            <solid android:color="#55919191" />

            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="2dp"
        android:right="1dp"
        android:top="2dp">
        <shape>
            <solid android:color="#88919191" />

            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:right="2dp">
        <shape>
            <solid android:color="#FFFFFF" />

            <gradient
                android:angle="90"
                android:endColor="#FFFFFF"
                android:startColor="#FFFFFF" />

            <stroke
                android:width="1dp"
                android:color="#33919191" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>


